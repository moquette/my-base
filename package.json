{
  "name": "my-base",
  "version": "1.0.24",
  "description": "My personal and heavily biased project base. Use at your own risk.",
  "keywords": [
    "react",
    "typescript",
    "webpack",
    "webpack 5",
    "webpack boilerplate",
    "tailwind",
    "tailwindcss"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:moquette/my-base.git"
  },
  "license": "MIT",
  "author": "Joaquin A. Moquette<moquette@gmail.com>",
  "main": "webpack.config.babel.js",
  "scripts": {
    "start": "cross-env WEBPACK_IS_DEV_SERVER=true NODE_ENV=development webpack serve --config webpack.config.babel.js",
    "build": "cross-env NODE_ENV=production webpack --config webpack.config.babel.js",
    "lint": "npx prettier './{src,webpack}/**/*.{js,jsx,ts,tsx,json,scss,css,md,html}' --write && yarn tsc --noEmit && npx eslint './src/**/*.{ts,tsx}' --fix",
    "profile": "yarn build && cross-env NODE_ENV=production webpack --profile --json --config webpack.config.babel.js > ./dist/profile.json && webpack-bundle-analyzer ./dist/profile.json",
    "clear": "npx rimraf .yarn/cache .yarn/install-state.gz .yarn/unplugged dist node_modules",
    "reset": "yarn clear && yarn install",
    "wrangler:deploy": "wrangler publish",
    "wrangler:dev": "wrangler dev --local",
    "postinstall": "husky install"
  },
  "dependencies": {
    "@cloudflare/kv-asset-handler": "~0.2.0",
    "@types/classnames": "^2.3.1",
    "@types/react": "^18.0.14",
    "@types/react-dom": "^18.0.5",
    "classnames": "^2.3.1",
    "git-branch-is": "^4.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-github-corner": "^2.5.0",
    "react-router-dom": "^6.3.0"
  },
  "devDependencies": {
    "@babel/core": "~7.18.6",
    "@babel/plugin-proposal-class-properties": "~7.18.6",
    "@babel/plugin-proposal-export-default-from": "^7.18.6",
    "@babel/plugin-proposal-export-namespace-from": "~7.18.6",
    "@babel/plugin-proposal-object-rest-spread": "~7.18.6",
    "@babel/plugin-proposal-throw-expressions": "~7.18.6",
    "@babel/plugin-syntax-dynamic-import": "~7.8.3",
    "@babel/plugin-transform-runtime": "~7.18.6",
    "@babel/preset-env": "~7.18.6",
    "@babel/preset-react": "~7.18.6",
    "@babel/register": "~7.18.6",
    "@cloudflare/workers-types": "^3.13.0",
    "@pmmmwh/react-refresh-webpack-plugin": "~0.5.7",
    "@teamsupercell/typings-for-css-modules-loader": "~2.5.1",
    "@types/jest": "^28.1.3",
    "@types/service-worker-mock": "^2.0.1",
    "@typescript-eslint/eslint-plugin": "~5.30.0",
    "@typescript-eslint/parser": "~5.30.0",
    "autoprefixer": "~10.4.7",
    "babel-eslint": "~10.1.0",
    "babel-loader": "~8.2.5",
    "clean-webpack-plugin": "~4.0.0",
    "copy-webpack-plugin": "~11.0.0",
    "core-js": "~3.23.3",
    "cross-env": "^7.0.3",
    "css-loader": "~6.7.1",
    "cssnano": "~5.1.12",
    "eslint": "~8.18.0",
    "eslint-config-airbnb-base": "~15.0.0",
    "eslint-config-airbnb-typescript": "~17.0.0",
    "eslint-config-prettier": "~8.5.0",
    "eslint-import-resolver-alias": "~1.1.2",
    "eslint-plugin-import": "~2.26.0",
    "eslint-plugin-jsx-a11y": "~6.6.0",
    "eslint-plugin-prettier": "^4.1.0",
    "eslint-plugin-react": "~7.30.1",
    "eslint-plugin-react-hooks": "~4.6.0",
    "eslint-webpack-plugin": "~3.2.0",
    "fork-ts-checker-webpack-plugin": "~7.2.11",
    "html-loader": "~3.1.2",
    "html-webpack-plugin": "~5.5.0",
    "husky": "^8.0.1",
    "import-sort-style-module-and-prefix": "~0.1.3",
    "less": "~4.1.3",
    "less-loader": "~11.0.0",
    "mini-css-extract-plugin": "~2.6.1",
    "path": "~0.12.7",
    "postcss": "~8.4.14",
    "postcss-import": "^14.1.0",
    "postcss-loader": "~7.0.0",
    "prettier": "~2.7.1",
    "prettier-plugin-import-sort": "~0.0.7",
    "prettier-plugin-tailwindcss": "^0.1.11",
    "react-refresh": "~0.14.0",
    "regenerator-runtime": "~0.13.9",
    "resolve-url-loader": "~5.0.0",
    "sass": "~1.53.0",
    "sass-loader": "~13.0.2",
    "sass-resources-loader": "~2.2.5",
    "service-worker-mock": "^2.0.5",
    "style-loader": "~3.3.1",
    "svg-url-loader": "~7.1.1",
    "tailwindcss": "^3.1.4",
    "terser-webpack-plugin": "~5.3.3",
    "ts-loader": "~9.3.1",
    "tsconfig-paths-webpack-plugin": "^3.5.2",
    "typescript": "~4.7.4",
    "url-loader": "~4.1.1",
    "webpack": "~5.73.0",
    "webpack-bundle-analyzer": "~4.5.0",
    "webpack-cli": "~4.10.0",
    "webpack-dev-server": "~4.9.2",
    "webpack-merge": "~5.8.0",
    "wrangler": "^2.0.15"
  },
  "importSort": {
    ".ts, .tsx": {
      "style": "module-and-prefix",
      "parser": "typescript"
    },
    ".js, .jsx": {
      "style": "module-and-prefix",
      "parser": "babylon"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "packageManager": "yarn@3.2.1"
}
